{
  "TelegramSettings": {
    "BotToken": "YOUR_BOT_TOKEN_HERE",
    "ApiId": "YOUR_API_ID_HERE",
    "ApiHash": "YOUR_API_HASH_HERE"
  },
  "ConnectionStrings": {
    "Redis": "localhost:6379",
    "Sqlite": "Data Source=/app/data/TelegramVerificationBot.sqlite"
  },
  "QuizFilePath": "/app/data/quizzes.json",
  "RateLimiting": {
    "FixedWindow": {
      "StartVerificationLimit": 3,
      "StartVerificationWindowSeconds": 60,
      "CallbackLimit": 10,
      "CallbackWindowSeconds": 60
    },
    "TokenBucket": {
      "StartVerificationCapacity": 5,
      "StartVerificationRefillRatePerSecond": 0.1,
      "CallbackCapacity": 20,
      "CallbackRefillRatePerSecond": 2
    },
    "LeakyBucket": {
      "StartVerificationIntervalSeconds": 20,
      "CallbackIntervalMilliseconds": 100
    }
  }
}
